public class LabP29 {

	public static void main(String[]args)
	{
		String state = "Mississipi";                                                     // Identifier declared Mississipi
		                                                                   
		System.out.print(state.replace("Mississipi", "M!$$!$$!p!"));             // All I(s) and S(s) replaced with ! and $  respectively
		
	}
	
}

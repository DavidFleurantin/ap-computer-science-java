import java.util.Random;

public class LabP27 {

	public static void main(String[] args) {
		
		Random gen = new Random();                                        // Random Class Declared
		
	System.out.println( gen.nextInt(6) +1 );                              // Generates random Number between  1 to 6
		
		
	}

}
